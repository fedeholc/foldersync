{
  "name": "foldersync",
  "publisher": "fedeholc",
  "displayName": "foldersync",
  "description": "Syncs files between folders based on configuration files.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:package.json",
    "workspaceContains:.git"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "foldersync.refreshView",
        "title": "foldersync: Refresh View"
      },
      {
        "command": "foldersync.openView",
        "title": "foldersync: Open View"
      },
      {
        "command": "foldersync.initialSyncLatest",
        "title": "foldersync: Initial Sync (Newest Wins)",
        "category": "foldersync"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "foldersync_container",
          "title": "foldersync",
          "icon": "resources/icons/filesync2.svg"
        }
      ]
    },
    "views": {
      "foldersync_container": [
        {
          "id": "foldersync.syncView",
          "name": "foldersync",
          "icon": "resources/icons/filesync2.svg"
        }
      ]
    },
    "view/item/context": {}
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/node": "22.x",
    "@types/mocha": "^10.0.10",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "mocha": "^10.8.2",
    "glob": "^11.0.0"
  }
}
